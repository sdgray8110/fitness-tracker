<tr class="activity-data {{ui.className}}" data-date="{{date.raw}}">
    <td class="expand-row"><button>Expand</button></td>
    <td>{{date.formatted}}</td>
    <td>{{distance.formatted}}</td>
    <td>{{duration.formatted}}</td>
    <td>{{climbing.formatted}}</td>
    <td>{{average_speed.formatted}}</td>
    <td>{{vam.formatted}}</td>
    <td>{{climbing_rate.formatted}}</td>
</tr>